{
  "name": "assertly",
  "version": "1.0.0",
  "description": "Assert class for BDD-style assertions",
  "main": "Assert.js",
  "scripts": {
    "debug": "devtool node_modules/mocha/bin/_mocha -qc --break -- ./test/specs/**/*.js",
    "debugx": "devtool node_modules/mocha/bin/_mocha -qc --break -- --compilers js:babel-core/register --require babel-polyfill ./test/specs/**/*.js",
    "docs": "node docs.js",
    "docs-debug": "devtool docs.js",
    "test": "mocha ./test/specs/**/*.js",
    "testx": "mocha --compilers js:babel-core/register --require babel-polyfill ./test/specs/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dongryphon/assertly.git"
  },
  "keywords": [
    "assert",
    "expect",
    "bdd",
    "testing"
  ],
  "author": "Don Griffin (dongryphon)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dongryphon/assertly/issues"
  },
  "homepage": "https://github.com/dongryphon/assertly#readme",
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-coveralls": "^0.1.4",
    "gulp-eslint": "^3.0.1",
    "gulp-exclude-gitignore": "^1.0.0",
    "gulp-istanbul": "^1.1.1",
    "gulp-mocha": "^3.0.1",
    "gulp-nsp": "^2.4.2",
    "gulp-plumber": "^1.1.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.1.0"
  },
  "devDeps-babel": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-polyfill": "^6.20.0"
  },
  "engines": {
    "node": ">=6.0.0"
  }
}
